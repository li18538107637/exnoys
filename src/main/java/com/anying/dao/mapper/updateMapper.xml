<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.anying.dao.UpdateDao">
	<update id="deleteData" parameterType="String">
		UPDATE FIORA_HIGHLANDER SET IS_OPEN ='1'
		WHERE ID=#{id}
	</update>
	<delete id="realDeleteData" parameterType="String">
		DELETE FROM FIORA_HIGHLANDER WHERE ID=#{id}

	</delete>
</mapper>